<h2>CreatePoll</h2>

@using (Html.BeginForm("SubmitPoll", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="form form-vertical usersection-div">
        <div class="row">
            <div class="col-sm-6 p-r-9">
                <div class="form-group">
                    <label for="Question">Question</label>
                    @Html.TextAreaFor(model => model.Questions, new { @class = "form-control", @id= "txtQuestion" })
                    @Html.HiddenFor(model => model.Questions, new { @id = "Question", })
                </div>
            </div>
            <div class="col-sm-6 p-l-9">
                <div class="form-group">
                    <label for="email">Options</label>
                    @Html.TextAreaFor(model => model.Options, new { @class = "form-control", @id = "txtOptions", @placeholder = "Option" })
                    @Html.HiddenFor(model => model.OptionA, new { @id = "Options", })
                    @*<input type="hidden" value="@HttpContext.Current.Session["UserNTID"].ToString()" id="hdEmployeeNTIDSessionVal" />*@
                </div>
            </div>           
            <div class="col-sm-6 p-l-9">
                <div class="form-group">
                    <label for="email">From Date</label>
                    @Html.TextBoxFor(model => model.FromDate, new { @class = "form-control", @id= "txtFromDate" })
                    @Html.HiddenFor(model => model.FromDate, new { @id = "FromDate", })
                </div>
            </div>
            <div class="col-sm-6 p-l-9">
                <div class="form-group">
                    <label for="email">To Date</label>
                    @Html.TextBoxFor(model => model.ToDate, new { @class = "form-control", @id= "txtToDate" })
                    @Html.HiddenFor(model => model.ToDate, new { @id = "ToDate", })
                </div>
            </div>

            <div class="col-sm-6 p-r-9">
                <div class="form-group">
                    <label for="email">IsActive</label>
                    @Html.CheckBoxFor(model => model.IsActive, new { @class = "form-control", @id="chbIsActive" })
                    @Html.HiddenFor(model => model.IsActive, new { @id = "IsActive", })
                </div>
            </div>
            <div class="col-sm-12 text-center m-t-20">
                <button type="submit" class="btn btn-success btn-sm regstrBtn" onclick="return ValidateManagePoll();">Create Poll</button>
            </div>
        </div>
    </div>
}

<script type="text/javascript">
    $(document).ready(function () {
        $("#txtFromDate").datepicker({
            minDate: "0"
        });
        $("#txtToDate").datepicker({
            minDate: "0"
        });
    });    
    function ValidateManagePoll() {
        var _Question = $("#txtQuestion").val();
        var _Options = $("#txtOptions").val();
        var _FromDate = $("#txtFromDate").val();
        var _ToDate = $("#txtToDate").val();
        var _ChkActive = $('#chbIsActive').is(':checked');
        if (_Question == "" || _Options == "" || _FromDate == "" || _ToDate == "" || _ChkActive == false) {
            alert("All fields are required.");
            return false;
        }
    }
</script>



