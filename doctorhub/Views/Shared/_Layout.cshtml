<body>
    <div class="top-orange">
        <div class="navbar navbar-inverse navbar-custom">
            <div class="container">
                <div class="row">
                    <div class="navbar-header col-sm-3">
                        <a class="navbar-brand" href="@Url.Action("Index", "Home")"><img src="~/images/hub-logo.png" alt="" /></a>
                    </div>
                    <div class="col-sm-5">
                        @*@Html.Partial("~/Views/Shared/_search.cshtml")*@
                    </div>
                    <div class="col-sm-4 right">
                        <ul class="nav navbar-nav navbar-right">
                            <li>
                                <a href="@Url.Action("FAQ","Common")">
                                    <img src="~/images/faq-icon.png" alt="" /><br>
                                    FAQ(S)
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("SubmitFeedBack","Home")">
                                    <img src="~/images/feedback-icon.png" alt="" /><br>
                                    Feedback
                                </a>
                            </li>
                            <li class="dropdown">
                                @*@Html.Partial("~/Views/MyProfile/_GetNotification.cshtml")*@
                                @*<ul class="dropdown-menu notify-drop" id="notiContent"></ul>*@
                                <ul class="dropdown-menu notify-drop" style=" font-size: 8pt;font-weight: normal;margin-top:5px;">
                                    <div class="drop-content" id="notiContent"></div>
                                    <div class="notify-drop-footer">
                                        <div class="col-md-9 col-sm-9 col-xs-9 text-right">
                                            <a href="/Home/AllNotification"><i class="fa fa-eye"></i><b> more notifications</b></a>
                                        </div>
                                        <div class="col-md-3 col-sm-3 col-xs-3 text-right"><a class="rIcon allRead closeNoti" data-tooltip="tooltip" data-placement="bottom" title="Close"><i class="fa fa-close"></i></a></div>
                                    </div>
                                </ul>

                            </li>
                            @foreach (var mp in menuList.Where(p => p.ParentId == 0 || p.ParentId == null))
                            {
                                var parent = menuList.Where(p => p.ParentId == mp.MenuId).ToList();

                                if (parent.Count > 0)
                                {
                                    <li class="dropdown">
                                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown"><img src="~/images/self-img-icon.png" /><br />@mp.MenuName<span class="caret"></span></a>
                                        <ul class="dropdown-menu">
                                            @foreach (var cp in menuList.Where(p => p.ParentId == mp.MenuId))
                                            {
                                                <li id="@mp.MenuName"><a href="/@cp.MenuUrl">@cp.MenuName</a></li>
                                            }
                                        </ul>
                                    </li>
                                }
                                else
                                {
                                    @*<li><a href="/@mp.MenuUrl">@mp.MenuName</a></li>*@
                                }
                            }
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="container">
        @RenderSection("featured", required: false)

        @RenderBody()
        <br />
        <br />
    </div>
    <div class="footer-bg">
        <div class="container">
            <p class="pull-left"><a href="#">Contact Us</a></p>
            <p class="pull-right"><img src="../images/ogs-logo.png" alt="" /></p>
        </div>
    </div>
    <!-- end footer -->

    <script src="~/Scripts/toastr.js"></script>
    <script src="~/Scripts/pnotify.custom.min.js"></script>

    <script>
        $(document).ready(function () {
            @{
                string actionName = this.ViewContext.RouteData.Values["action"].ToString();
                string controllerName= this.ViewContext.RouteData.Values["controller"].ToString();
            }
                switch ("@controllerName") {
                    case "Home": case "home":
                        $("#mnuHome").addClass('active');
                        break;
                    case "Directory": case "directory":
                        $("#Directory").addClass('active');
                        break;
                    case "Admin": case "admin":
                        $("#Admin").addClass('active');
                        break;
                default:
                        $("#mnuHome").addClass('active');
                    break;
            }
        });

        $('.closeNoti').click(function () {
            $('.notify-drop').hide();
        });

        var error = '@TempData["error"]';
        var success = '@TempData["success"]';
        var warning = '@TempData["warning"]';
        var registrationSuccess = '@TempData["registerSuccess"]';
        var statusPending = '@TempData["statusPending"]';
        var alreadyRegistration = '@TempData["userExist"]';

        if (error != null && error != undefined && error != '')
            toasterErrorMessage(error);

        if (success != null && success != undefined && success != '')
            toasterSuccessMessage(success);

        if (warning != null && warning != undefined && warning != '')
            toasterWarningMessage(warning);

        if (registrationSuccess != null && registrationSuccess != undefined && registrationSuccess != '')
            RegistrationSuccess(registrationSuccess);

        if (statusPending != null && statusPending != undefined && statusPending != '')
            UserActivation(statusPending);

        if (alreadyRegistration != null && alreadyRegistration != undefined && alreadyRegistration != '')
            UserExist(alreadyRegistration);

        function toasterSuccessMessage(message) {
            new PNotify({
                title: 'Success',
                text: message,
                type: 'success',
                styling: 'bootstrap3'
            });
        }
        function toasterErrorMessage(message) {
            new PNotify({
                title: 'Error',
                text: message,
                type: 'error',
                styling: 'bootstrap3'
            });
        }
        function toasterWarningMessage(message) {
            new PNotify({
                title: 'Warning',
                text: message,
                styling: 'bootstrap3'
            });
        }
        function toasterInfoMessage(message) {
            new PNotify({
                title: 'Info',
                text: message,
                type: 'info',
                styling: 'bootstrap3'
            });
        }
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
