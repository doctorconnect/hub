    List<doctorhubBusinessEntities.Menu> menuList = new doctorhubDataAccess.DirectoryDataAccess().GetMenuList(Convert.ToString(HttpContext.Current.Session["UserNTID"]));
    }
<html>

<head>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
