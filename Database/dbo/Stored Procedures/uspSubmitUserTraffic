
CREATE  PROCEDURE [dbo].[uspSubmitUserTraffic]  
(  		 
 @CreatedOn DATETIME,
 @CapId int                 
)                                                      
AS                                                      
BEGIN        
    BEGIN TRANSACTION;      
      SAVE TRANSACTION MySavePoint;        
     BEGIN TRY
	 BEGIN               
    INSERT INTO [KMTUserTraffic] (CreatedOn,CapId)
	   values (@CreatedOn,@CapId);	
		END          			                    
     END TRY       
     BEGIN CATCH      
        IF @@TRANCOUNT > 0      
        BEGIN      
            ROLLBACK TRANSACTION MySavePoint;       
        END      
      END CATCH                                                 
     COMMIT TRANSACTION                                                  
END
GO
