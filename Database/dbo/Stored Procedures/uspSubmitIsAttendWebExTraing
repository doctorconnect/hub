
CREATE  PROCEDURE [dbo].[uspSubmitIsAttendWebExTraing]  
(  	
 
 @TraingId  int,		 
 @TraingTitle  VARCHAR(50),
 @CreatedBy VARCHAR(50),
 @CreatedOn DATETIME                      
)                                                      
AS                                                      
BEGIN        
    BEGIN TRANSACTION;      
      SAVE TRANSACTION MySavePoint;        
     BEGIN TRY
	BEGIN               
    INSERT INTO KMTWebExTraing (TraingId,TraingTitle,CREATEDBY,CREATEDON)
	   VALUES (@TraingId,@TraingTitle,@CreatedBy,@CreatedOn);		
		END	       			                    
     END TRY       
     BEGIN CATCH      
        IF @@TRANCOUNT > 0      
        BEGIN      
            ROLLBACK TRANSACTION MySavePoint;       
        END      
      END CATCH                                                 
     COMMIT TRANSACTION                                                  
END

GO
